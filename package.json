{
  "name": "charts-comparison",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "recharts-project",
    "nivo-project",
    "ant-charts-project"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:all": "npm run build -w recharts-project && npm run build -w nivo-project && npm run build -w ant-charts-project && npm run compare",
